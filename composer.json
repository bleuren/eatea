{
    "name": "laravel/laravel",
    "type": "project",
    "description": "The Laravel Framework.",
    "keywords": [
        "framework",
        "laravel"
    ],
    "license": "MIT",
    "require": {
        "php": "^7.3|^8.0",
        "ext-exif": "*",
        "ext-gd": "*",
        "asantibanez/livewire-calendar": "^2.1",
        "bavix/laravel-wallet": "^6.1",
        "bleuren/voyager-bread-generator": "^1.1",
        "bumbummen99/shoppingcart": "^4.0",
        "fakerphp/faker": "dev-add-datebetween-support",
        "fideloper/proxy": "^4.4",
        "fruitcake/laravel-cors": "^2.0",
        "guzzlehttp/guzzle": "^7.0.1",
        "laravel/framework": "^8.40",
        "laravel/jetstream": "^2.3",
        "laravel/sanctum": "^2.6",
        "laravel/socialite": "^5.2",
        "laravel/tinker": "^2.5",
        "livewire/livewire": "^2.5",
        "phattarachai/line-notify": "dev-eatea",
        "questocat/laravel-referral": "^1.0",
        "rappasoft/laravel-livewire-tables": "^1.10",
        "socialiteproviders/line": "^4.1",
        "tcg/voyager": "^1.4",
        "yish/imgur": "^2.0"
    },
    "require-dev": {
        "facade/ignition": "^2.5",
        "laravel/sail": "^1.0.1",
        "mockery/mockery": "^1.4.2",
        "nunomaduro/collision": "^5.0",
        "phpunit/phpunit": "^9.3.3"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "Database\\Factories\\": "database/factories/",
            "Database\\Seeders\\": "database/seeders/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ],
        "post-install-cmd": [
            "ln -sr storage/app/public public/storage"
        ]
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "repositories": {
        "hooks": {
            "type": "composer",
            "url": "https://larapack.io"
        },
        "pleuren/line-notify": {
            "type": "vcs",
            "url": "https://github.com/bleuren/line-notify"
        },
        "pleuren/faker": {
            "type": "vcs",
            "url": "https://github.com/bleuren/Faker"
        }
    }
}